<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>OnTheFly</display-name>
  <welcome-file-list>
  	<welcome-file>login.jsp</welcome-file>
    <welcome-file>generateUsageReport.jsp</welcome-file>
    <welcome-file>usagedashboard.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <display-name>GenerateUsageReportController</display-name>
    <servlet-name>GenerateUsageReportController</servlet-name>
    <servlet-class>controller.GenerateUsageReportController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GenerateUsageReportController</servlet-name>
    <url-pattern>/GenerateUsageReportController</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>LoginController</display-name>
    <servlet-name>LoginController</servlet-name>
    <servlet-class>controller.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginController</servlet-name>
    <url-pattern>/LoginController</url-pattern>
  </servlet-mapping>
</web-app>